{
  "name": "svenjungnickel/redactor-font-awesome",
  "description": "Craft redactor font awesome plugin for Craft 3",
  "type": "craft-plugin",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "redactorfontawesome"
  ],
  "support": {
    "docs": "https://github.com/svenjungnickel/craft-redactor-font-awesome/blob/master/README.md",
    "issues": "https://github.com/svenjungnickel/craft-redactor-font-awesome/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Sven Jungnickel",
      "homepage": "https://github.com/svenjungnickel"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "craftcms/cms": "^3.0.0 | ^4.0.0",
    "craftcms/redactor": "^2.0 | ^3.0"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "svenjungnickel\\redactorfontawesome\\": "src/"
    }
  },
  "extra": {
    "name": "Redactor Font Awesome",
    "handle": "redactor-font-awesome",
    "developer": "Sven Jungnickel",
    "developerUrl": "https://github.com/svenjungnickel/",
    "schemaVersion": "2.0",
    "description": "Craft redactor font awesome plugin for Craft 3",
    "hasCpSettings": false,
    "hasCpSection": false,
    "documentationUrl": "https://github.com/svenjungnickel/craft-redactor-font-awesome/blob/master/README.md",
    "changelogUrl": "https://github.com/svenjungnickel/craft-redactor-font-awesome/blob/master/CHANGELOG.md",
    "class": "svenjungnickel\\redactorfontawesome\\RedactorFontAwesome"
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": false,
      "yiisoft/yii2-composer": false
    }
  }
}
